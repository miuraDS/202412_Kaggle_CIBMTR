    "\n",
    "# '男平均', '女平均', '全平均'の累積和カラムを作成\n",
    "death_rate_df['男平均_累積和'] = death_rate_df['男平均'].cumsum()\n",
    "death_rate_df['女平均_累積和'] = death_rate_df['女平均'].cumsum()\n",
    "death_rate_df['全平均_累積和'] = death_rate_df['全平均'].cumsum()\n",
    "\n",
    "# データフレームを表示\n",
    "display(death_rate_df)"
